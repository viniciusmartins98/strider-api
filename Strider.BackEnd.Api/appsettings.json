{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "Cors": {
    "Methods": [
      "GET",
      "PUT",
      "PATCH",
      "POST",
      "DELETE"
    ],
    "Origins": [
      "http://localhost:5173"
    ],
    "Headers": [
      "Origin",
      "X-Requested-With",
      "Content-Type",
      "Accept",
      "Authorization",
      "X-Access-Token"
    ],
    "ExposedHeaders": [
      "X-Access-Token"
    ]
  }
}